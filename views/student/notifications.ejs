<%- include('../partials/header') %>
<%- include('../partials/nav') %>
<h1>Notifications</h1>

<ul>
<% if(notifications.length > 0) { %>
    <% notifications.forEach((notification) => {
        switch(notification.__t) {
            case "FollowNotif": %>
            <li>
               <a href="/student/view/<%= notification.senderUserId %>"><%= notification.message %> follows</a>
            </li>
            <% break; %>
            <% } %>
        <% }) %>
<% } else { %>
    <p>No new notificationss</p>
<% } %>
</ul>

<%- include('../partials/footer') %>