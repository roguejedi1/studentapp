<%- include('../partials/header') %>
<%- include('../partials/nav') %>
<h1>Notifications</h1>

<ul>
<% if(notifications.length > 0) { %>
    <% notifications.forEach((notification) => { %>
            <li>
               <a href="/student/view/<%= notification.senderUserId %>"><%= notification.message %></a>
            </li>
        <% }) %>
<% } else { %>
    <p>No new notificationss</p>
<% } %>
</ul>

